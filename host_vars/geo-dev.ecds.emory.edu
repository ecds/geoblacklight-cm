# vars file for dce_role_provision_ec2
# EC2 instance variables
# project_name
# env_name
project_name: geoblacklight

ec2_key_name: "{{ vault_ec2_key_name }}"
ec2_security_groups: [Griendling Site Security Group]
ec2_instance_type: t2.medium
ec2_image: ami-6871a115
ansible_ssh_user: ec2-user

ec2_vpc_subnet_id: subnet-ba79ee80
ec2_assign_public_ip: no #Boolean

ec2_fqdn: geo-dev.ecds.emory.edu


## Geoblacklight Capdeploy files
rails_host_fqdn: "geo-dev.ecds.emory.edu"
enable_ssl: "no"
rails_db_adapter: "mysql2"
rails_db_pool: "5"
rails_db_host: "{{ rds_endpoint }}"
rails_db_name: "{{ vault_dev_rails_db_name }}"
rails_db_user: "{{ vault_dev_rails_db_user }}"
rails_db_pass: "{{ vault_dev_rails_db_pass }}"

solr_index_url: "http://52.91.245.168:9080/solr/geoblacklight1"

sidekiq_num_threads: "5"


## ECDS Git
git_repo_url: "https://github.com/ecds/geoblacklight-2nd"
git_repo_branch: "use-ansible-scripts"
